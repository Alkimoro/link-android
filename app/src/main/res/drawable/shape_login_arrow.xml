<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:right="6dp" android:left="6dp">
        <shape>
            <solid android:color="@android:color/white"/>
            <corners android:radius="5dp"/>
            <size android:width="50dp" android:height="7dp"/>
        </shape>
    </item>
    <item android:left="27dp" android:right="6dp">
        <rotate android:pivotX="65" android:pivotY="50%" android:toDegrees="0"
            android:fromDegrees="-45">
            <shape>
                <solid android:color="@android:color/white"/>
                <corners android:radius="5dp"/>
            </shape>
        </rotate>
    </item>
    <item android:left="27dp" android:right="6dp">
        <rotate android:pivotX="65" android:pivotY="50%" android:toDegrees="0"
            android:fromDegrees="45">
            <shape>
                <solid android:color="@android:color/white"/>
                <corners android:radius="5dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>